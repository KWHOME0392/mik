<view class="lunbo">
  <swiper indicator-dots="{{indicatorDots}}" circular="{{circular}}"
    autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
    <block wx:for="{{bannerData}}" wx:key="id">
      <swiper-item>
        <image src="{{item.imgUrl}}" class="slide-image"/>
      </swiper-item>
    </block>
  </swiper>
</view>